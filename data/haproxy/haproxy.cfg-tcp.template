global
  log stdout format raw local0 debug

defaults 
  mode tcp
  log global
  timeout http-request 10s
  timeout connect 5s
  timeout client 10s
  timeout server 10s

frontend "${APP}"
  mode tcp
  bind *:"${PORT}" name "${APP}"
  default_backend "${APP}"

backend "${APP}"
  mode tcp
  server "${APP}" "${APP}":"${PRIVATE_PORT}" check
